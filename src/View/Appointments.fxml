<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.Appointments">
    <children>
        <Button layoutX="34.0" layoutY="24.0" mnemonicParsing="false" onAction="#onActionBack" text="Back" />
        <Label layoutX="108.0" layoutY="19.0" text="Appointments">
            <font>
                <Font size="31.0" />
            </font>
        </Label>
        <Button layoutX="31.0" layoutY="522.0" mnemonicParsing="false" onAction="#onActionAddAppointment" text="Add">
            <font>
                <Font size="24.0" />
            </font>
        </Button>
        <Button layoutX="328.0" layoutY="522.0" mnemonicParsing="false" onAction="#onActionDeleteAppointment" text="Delete">
            <font>
                <Font size="24.0" />
            </font>
        </Button>
        <Button layoutX="144.0" layoutY="522.0" mnemonicParsing="false" onAction="#onActionModifyAppointment" text="Update">
            <font>
                <Font size="24.0" />
            </font>
        </Button>
        <TableView fx:id="appointmentsTableView" layoutX="31.0" layoutY="103.0" prefHeight="401.0" prefWidth="395.0">
            <columns>
                <TableColumn maxWidth="750.0" minWidth="30.0" prefWidth="75.0" text="C1" />
                <TableColumn maxWidth="750.0" minWidth="30.0" prefWidth="75.0" text="C2" />
            </columns>
        </TableView>
        <RadioButton layoutX="48.0" layoutY="70.0" mnemonicParsing="false" text="Current Week" />
        <RadioButton layoutX="169.0" layoutY="70.0" mnemonicParsing="false" text="Current Month" />
        <RadioButton layoutX="295.0" layoutY="70.0" mnemonicParsing="false" text="All Appointments" />
    </children>
</AnchorPane>
